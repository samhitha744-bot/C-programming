#include <stdio.h>

int main() {

    int a = 7, b = 7;

    // Arithmetic Operators
    printf("Arithmetic Operators\n");
    printf("a + b = %d\n", a + b);
    printf("a - b = %d\n", a - b);
    printf("a * b = %d\n", a * b);
    printf("a / b = %d\n", a / b);
    printf("a % b = %d\n\n", a % b);

    // Relational Operators
    printf("Relational Operators\n");
    printf("a > b = %d\n", a > b);
    printf("a < b = %d\n", a < b);
    printf("a == b = %d\n\n", a == b);

    // Logical Operators
    printf("Logical Operators\n");
    printf("(a > b) && (a == b) = %d\n", (a > b) && (a == b));
    printf("(a > b) || (a == b) = %d\n", (a > b) || (a == b));
    printf("!(a == b) = %d\n\n", !(a == b));

    // Assignment Operators
    printf("Assignment Operators\n");
    
    printf("a += b → %d\n", a);
printf("a -= b → %d\n", a);
printf("a *= b → %d\n", a);
printf("a /= b → %d\n", a);
printf("a %= b → %d\n\n", a);

    // Increment & Decrement
    printf("\nIncrement and Decrement\n");
    printf("++b = %d\n", ++b);
    printf("--b = %d\n", --b);
    printf("b++ = %d\n", b++);
    printf("b--= %d\n", b--);
    return 0;
}

    
